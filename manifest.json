{
  "manifest_version": 3,
  "name": "LinkedIn Auto Connect",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/mynetwork/grow/"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["dist/bundle.js"],
      "matches": ["https://www.linkedin.com/*"]
    }
  ]
}
